  <a class="btn btn-danger" href="/vehiculos">Regresar al Menu Vehiculos</a>
  <div class="container">
  <form action="/vehiculos/editar/{{vehiculo.id}}" method="POST">
    <table class="table">
      <tbody>
        <tr>
          <td>
